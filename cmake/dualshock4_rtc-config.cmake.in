set(DUALSHOCK4_RTC_DEFINITIONS ${<dependency>_DEFINITIONS})

set(DUALSHOCK4_RTC_VERSION @PROJECT_VERSION@)
set(DUALSHOCK4_RTC_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
